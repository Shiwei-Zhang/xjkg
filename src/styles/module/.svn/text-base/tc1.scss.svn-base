.tcContent {
    width: 4.97rem;
    height: 100%;
    margin: 0 1.34rem;
    position: relative;
    .borderSolid {
        width: 0.83rem;
        height: 0.83rem;
        transform: rotate(135deg);
        -webkit-transform-origin: center center;
        transform-origin: center center;
        border-radius: 0.04rem;
        border: 0.01rem solid #3cd6f7;
        /* position: absolute;
     top: 0;
     left: 0;*/
    }
    .borderDashed {
        width: 0.83rem;
        height: 0.83rem;
        //          margin-top: 1.73rem;
        transform: rotate(135deg);
        -webkit-transform-origin: center center;
        transform-origin: center center;
        border-radius: 0.04rem;
        border: 1px dashed #3cd6f7;
        /* position: absolute;
     top: 0;
     left: 0;*/
    }
    .nodeBox {
        position: absolute;
        left: 0.2rem;
        font-size: 0.12rem;
        .nodeText {
            position: absolute;
            top: 0.2rem;
            left: -0.1rem;
            display: inline-block;
            color: #3cd6f7;
            height: 0.3rem;
            width: 1rem;
            word-break: break-all;
            word-wrap: break-word;
            line-height: 0.22rem;
            text-align: center;
        }
    }
    .nodeBox1 {
        top: 0.3rem;
        left: 0.2rem;
    }
    .nodeBox2 {
        top: 2rem;
    }
    .nodeBox3 {
        top: 3.7rem;
    }
    .nodeBox4 {
        top: 8.4rem;
    }
    .nodeBox5 {
        top: 6.5rem;
    }
    .nodeBox6 {
        top: 10.5rem;
    }
    .nodeBox7 {
        top: 15rem;
    }
    .nodeBox8 {
        top: 16.8rem;
    }
}

